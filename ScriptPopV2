-- Variables to store toggle state and search term
local toggleState = false
local searchTerm = ""

-- Function to equip a specific item
function equipItem(itemName)
    local args = {
        [1] = itemName
    }
    game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("Equip"):FireServer(unpack(args))
    print("Equipped item: " .. itemName)
end

-- Function to drop a specific item
function dropItem(itemName)
    local args = {
        [1] = itemName
    }
    game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("Drop"):FireServer(unpack(args))
    print("Dropped item: " .. itemName)
end

-- Function to continuously equip and drop a specific item
function equipAndDropItem()
    while toggleState do
        equipItem(searchTerm)
        wait(0.2) -- Decreased delay to make the process faster
        dropItem(searchTerm)
        wait(0.2) -- Decreased delay to make the process faster
    end
end

-- Create a tab and input field for item search
local droptab = DrRayLibrary.newTab("Credits", "ImageIdHere")
droptab.newInput("Item", "Type an item name to equip and drop", function(text)
    searchTerm = text
    print("Search term set to: " .. searchTerm)
end)

-- Create a toggle to start/stop the equip and drop item loop
droptab.newToggle("Fast Drop", "Drops Searched item very fast", false, function(state)
    toggleState = state
    if toggleState then
        print("Toggle On")
        if searchTerm ~= "" then
            spawn(equipAndDropItem) -- Start the loop in a new thread if search term is set
        else
            print("No search term set!")
        end
    else
        print("Toggle Off")
    end
end)

print("Script Loaded Successfully")

