local autotab = DrRayLibrary.newTab("Auto Buy", "ImageIdHere")
autotab.newToggle("Auto Buy All Stranger Things items", "Auto Buys All Stranger Things items", false, function(toggleState)
    getgenv().togglestrange = toggleState
    if toggleState then
        spawn(ToggleStrange)
    end
end)
autotab.newToggle("Auto Buy All letters", "Auto Buys All Letters", false, function(toggleState)
    getgenv().toggleletter = toggleState
    if toggleState then
        spawn(ToggleLetter)
    end
end)
autotab.newToggle("Auto Buy All Code items", "Auto Buys All code stuff", false, function(toggleState)
    getgenv().togglecode = toggleState
    if toggleState then
        spawn(ToggleCode)
    end
end)
autotab.newToggle("Auto Buy All box items", "Auto Buys All box stuff", false, function(toggleState)
    getgenv().togglebox = toggleState
    if toggleState then
        spawn(ToggleBox)
    end
end)
autotab.newToggle("Auto Buy All heart items", "Auto Buys All heart stuff", false, function(toggleState)
    getgenv().toggleheart = toggleState
    if toggleState then
        spawn(ToggleHeart)
    end
end)
autotab.newToggle("Auto Buy All Yellow items", "Auto Buys All Yellow stuff", false, function(toggleState)
    getgenv().toggleyellow = toggleState
    if toggleState then
        spawn(ToggleYellow)
    end
end)

